{
  "name": "ext_poc",
  "description": "VSCode extension POC",
  "version": "0.0.1",
  "publisher": "slowgen",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlowGen/ext_poc"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onWebviewView:extPoc.counter"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "icon": "web/icons/Icon-192.png",
          "id": "extPoc.counter",
          "name": "Flutter Counter",
          "type": "webview"
        }
      ]
    },
    "commands": [
      {
        "command": "extPoc.addOne",
        "category": "Counter Button",
        "title": "Add One",
        "icon": "$(add)"
      },
      {
        "command": "extPoc.reset",
        "category": "Counter Button",
        "title": "Reset to zero",
        "icon": "$(clear-all)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "extPoc.addOne",
          "group": "navigation",
          "when": "view == extPoc.counter"
        },
        {
          "command": "extPoc.reset",
          "group": "navigation",
          "when": "view == extPoc.counter"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "scripts/compile.sh",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "typescript": "^5.1.3"
  }
}